{
  "name": "sapper-postcss-template",
  "description": "A template that includes Sapper for Svelte and PostCSS preprocessing with Tailwind CSS",
  "keywords": [
    "sapper",
    "eslint",
    "postcss",
    "purgecss",
    "svelte",
    "tailwindcss",
    "cssnano"
  ],
  "homepage": "https://github.com/babichjacob/sapper-postcss-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/babichjacob/sapper-postcss-template.git"
  },
  "license": "MIT",
  "version": "2020.07.17",
  "scripts": {
    "eslint": "eslint",
    "eslint:fix": "eslint --fix ./*.js ./src/*.js ./src/routes/**/*.svelte ./src/components/**/*.svelte",
    "dev": "sapper dev",
    "build": "cross-env NODE_ENV=production sapper build --legacy",
    "prod": "npm run build",
    "export": "cross-env NODE_ENV=production sapper export --legacy",
    "start": "node __sapper__/build"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "sirv": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@tailwindcss/ui": "^0.3.1",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "postcss": "^7.0.32",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.21.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.27.16",
    "svelte": "^3.24.0",
    "svelte-preprocess": "^4.0.8",
    "tailwindcss": "^1.5.1"
  }
}
